{
  "name": "ro-fitness-app",
  "version": "0.1.0",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.6.7",
    "nodemailer": "^6.3.0",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "sendmail": "^1.6.1",
    "sequelize": "^5.10.3"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "concurrently \"nodemon --ignore 'client-side-application/*'\" \"npm run client\"",
    "start:prod": "node index.js",
    "build": "react-scripts build",
    "client": "cd client-side-application && npm run start",
    "install": "cd client-side-application && npm install",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "author": "",
  "license": "ISC"
}
